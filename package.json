{
  "name": "iamsank8",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "setup": "chmod +x dev-scripts/*.sh && ./dev-scripts/setup.sh",
    "start:all": "chmod +x dev-scripts/*.sh && ./dev-scripts/start-all.sh",
    "deploy": "chmod +x dev-scripts/*.sh && ./dev-scripts/deploy.sh",
    "deploy:staging": "chmod +x dev-scripts/*.sh && ./dev-scripts/deploy.sh staging",
    "db:seed": "chmod +x dev-scripts/*.sh && ./dev-scripts/db-operations.sh seed",
    "db:backup": "chmod +x dev-scripts/*.sh && ./dev-scripts/db-operations.sh backup",
    "db:restore": "chmod +x dev-scripts/*.sh && ./dev-scripts/db-operations.sh restore",
    "db:clear": "chmod +x dev-scripts/*.sh && ./dev-scripts/db-operations.sh clear",
    "upgrade": "chmod +x upgrade-angular.sh && ./upgrade-angular.sh",
    "make-scripts-executable": "chmod +x dev-scripts/*.sh && chmod +x upgrade-angular.sh",
    "setup:theme": "chmod +x dev-scripts/setup-theme.sh && ./dev-scripts/setup-theme.sh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.12",
    "@angular/cdk": "^16.2.14",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.3.12",
    "@angular/core": "^17.3.12",
    "@angular/forms": "^17.3.12",
    "@angular/material": "^16.2.14",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/router": "^17.3.12",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.17",
    "@angular/cli": "^17.3.17",
    "@angular/compiler-cli": "^17.3.12",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~5.4.5"
  }
}