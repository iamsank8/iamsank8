{
  "name": "iamsank8",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "setup": "chmod +x dev-scripts/*.sh && ./dev-scripts/setup.sh",
    "start:all": "chmod +x dev-scripts/*.sh && ./dev-scripts/start-all.sh",
    "deploy": "chmod +x dev-scripts/*.sh && ./dev-scripts/deploy.sh",
    "deploy:staging": "chmod +x dev-scripts/*.sh && ./dev-scripts/deploy.sh staging",
    "db:seed": "chmod +x dev-scripts/*.sh && ./dev-scripts/db-operations.sh seed",
    "db:backup": "chmod +x dev-scripts/*.sh && ./dev-scripts/db-operations.sh backup",
    "db:restore": "chmod +x dev-scripts/*.sh && ./dev-scripts/db-operations.sh restore",
    "db:clear": "chmod +x dev-scripts/*.sh && ./dev-scripts/db-operations.sh clear",
    "upgrade": "chmod +x upgrade-angular.sh && ./upgrade-angular.sh",
    "make-scripts-executable": "chmod +x dev-scripts/*.sh && chmod +x upgrade-angular.sh",
    "setup:theme": "chmod +x dev-scripts/setup-theme.sh && ./dev-scripts/setup-theme.sh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.3.0",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/forms": "^14.3.0",
    "@angular/material": "^13.3.9",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/router": "^14.3.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.13",
    "@angular/cli": "^14.2.13",
    "@angular/compiler-cli": "^14.3.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.6.2"
  }
}