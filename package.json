{
  "name": "iamsank8",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:github": "ng build --configuration=github-pages --base-href=/iamsank8/",
    "watch": "ng build --watch --configuration development",
    "setup": "chmod +x dev-scripts/*.sh && ./dev-scripts/setup.sh",
    "start:all": "chmod +x dev-scripts/*.sh && ./dev-scripts/start-all.sh",
    "deploy": "chmod +x dev-scripts/*.sh && ./dev-scripts/deploy.sh",
    "deploy:github": "npm run build:github && mkdir -p dist/iamsank8/assets && cp -r data/* dist/iamsank8/assets/ && echo 'GitHub Pages build ready in dist/iamsank8'",
    "deploy:staging": "chmod +x dev-scripts/*.sh && ./dev-scripts/deploy.sh staging",
    "db:seed": "chmod +x dev-scripts/*.sh && ./dev-scripts/db-operations.sh seed",
    "db:backup": "chmod +x dev-scripts/*.sh && ./dev-scripts/db-operations.sh backup",
    "db:restore": "chmod +x dev-scripts/*.sh && ./dev-scripts/db-operations.sh restore",
    "db:clear": "chmod +x dev-scripts/*.sh && ./dev-scripts/db-operations.sh clear",
    "data:seed": "node scripts/data-manager.js seed",
    "data:backup": "node scripts/data-manager.js backup",
    "data:restore": "node scripts/data-manager.js restore",
    "data:clear": "node scripts/data-manager.js clear",
    "data:list": "node scripts/data-manager.js list",
    "data:count": "node scripts/data-manager.js count",
    "make-scripts-executable": "chmod +x dev-scripts/*.sh",
    "setup:theme": "chmod +x dev-scripts/setup-theme.sh && ./dev-scripts/setup-theme.sh",
    "setup:github-secrets": "chmod +x scripts/setup-github-secrets.sh && ./scripts/setup-github-secrets.sh",
    "lint": "ng lint",
    "test:coverage": "ng test --watch=false --code-coverage --browsers=ChromeHeadless",
    "e2e": "cypress open",
    "e2e:headless": "cypress run",
    "format": "prettier --write \"src/**/*.{ts,html,scss,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,scss,css,json}\"",
    "quality:report": "node scripts/generate-quality-report.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.12",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.3.12",
    "@angular/core": "^17.3.12",
    "@angular/forms": "^17.3.12",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/router": "^17.3.12",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primeng": "^17.18.11",
    "rxjs": "^7.8.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.17",
    "@angular-devkit/build-webpack": "^0.2003.3",
    "@angular/cli": "^17.3.17",
    "@angular/compiler-cli": "^17.3.12",
    "@types/jasmine": "^5.1.13",
    "@types/node": "^12.11.1",
    "angular-eslint": "20.6.0",
    "cypress": "^15.7.0",
    "dotenv": "^17.2.2",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "firebase-admin": "^13.5.0",
    "jasmine-core": "^5.12.1",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "prettier": "^3.6.2",
    "typescript": "~5.4.5",
    "typescript-eslint": "8.46.3",
    "webpack": "^5.101.3"
  }
}